{
    "schemes": [
        "http"
    ],
    "swagger": "2.0",
    "info": {
        "description": "API сервер для управления логистикой",
        "title": "TechLogist API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8400",
    "basePath": "/api",
    "paths": {
        "/api/check": {
            "get": {
                "description": "Проверка подключения к сервису",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Check"
                ],
                "summary": "Проверка подключения к сервису",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/model.CheckResponse"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/ident/users/{id}": {
            "get": {
                "description": "Возвращает информацию о пользователе по его ID (int или UUID)",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Identity"
                ],
                "summary": "Получить пользователя по ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "ID пользователя (число или UUID)",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Информация о пользователе",
                        "schema": {
                            "$ref": "#/definitions/model.TL_identety_ModelUser"
                        }
                    },
                    "400": {
                        "description": "Неверный формат ID",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Пользователь не найден",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            },
            "patch": {
                "description": "Обновляет информацию о пользователе по его ID (int или UUID)",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Identity"
                ],
                "summary": "Обновить данные пользователя",
                "parameters": [
                    {
                        "type": "string",
                        "description": "ID пользователя (число или UUID)",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Данные для обновления",
                        "name": "user",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.UserUpdate"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Пользователь успешно обновлен",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Неверный формат ID",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Пользователь не найден",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Ошибка обновления пользователя",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/orders": {
            "post": {
                "description": "Создает новую заявку в системе",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Orders"
                ],
                "summary": "Создать новую заявку",
                "parameters": [
                    {
                        "description": "Данные для создания заявки",
                        "name": "order",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.OrderCreate"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Заявка успешно создана",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Неверные данные заявки",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Ошибка создания заявки",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/orders/actual": {
            "get": {
                "description": "Возвращает список всех актуальных заявок",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Orders"
                ],
                "summary": "Получить все актуальные заявки",
                "parameters": [
                    {
                        "type": "boolean",
                        "default": false,
                        "description": "Приватные заявки",
                        "name": "isPrivate",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Список актуальных заявок",
                        "schema": {
                            "$ref": "#/definitions/model.OrdersResponse"
                        }
                    },
                    "500": {
                        "description": "Ошибка получения заявок",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/orders/{id}": {
            "get": {
                "description": "Возвращает информацию о заявке по её ID",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Orders"
                ],
                "summary": "Получить заявку по ID",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "ID заявки",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Информация о заявке",
                        "schema": {
                            "$ref": "#/definitions/model.Order"
                        }
                    },
                    "400": {
                        "description": "Неверный формат ID",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Заявка не найдена",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Ошибка получения заявки",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/orders/{id}/accept": {
            "patch": {
                "description": "Принимает заявку водителем",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Orders"
                ],
                "summary": "Принять заявку",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "ID заявки",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Заявка успешно принята",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Неверный формат ID",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Заявка не найдена",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Ошибка принятия заявки",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/orders/{id}/cancel": {
            "patch": {
                "description": "Отменяет заявку",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Orders"
                ],
                "summary": "Отменить заявку",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "ID заявки",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Заявка успешно отменена",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Неверный формат ID",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Заявка не найдена",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Ошибка отмены заявки",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/orders/{id}/complete": {
            "patch": {
                "description": "Завершает заявку",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Orders"
                ],
                "summary": "Завершить заявку",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "ID заявки",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Заявка успешно завершена",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Неверный формат ID",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Заявка не найдена",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Ошибка завершения заявки",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/orders/{id}/reject": {
            "patch": {
                "description": "Отклоняет заявку водителем",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Orders"
                ],
                "summary": "Отклонить заявку",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "ID заявки",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Заявка успешно отклонена",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Неверный формат ID",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Заявка не найдена",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Ошибка отклонения заявки",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/public/auth/login": {
            "post": {
                "description": "Входит в систему",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Identity"
                ],
                "summary": "Входит в систему",
                "parameters": [
                    {
                        "description": "Данные для входа",
                        "name": "user",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.LoginRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Пользователь успешно введен",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Неверные данные пользователя",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Ошибка входа в систему",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/public/auth/register": {
            "post": {
                "description": "Создает нового пользователя в системе",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Identity"
                ],
                "summary": "Зарегистрировать нового пользователя",
                "parameters": [
                    {
                        "description": "Данные для регистрации",
                        "name": "user",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.RegisterRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Пользователь успешно создан",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Неверные данные пользователя",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Ошибка создания пользователя",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "model.CheckResponse": {
            "description": "DTO для передачи роли на фронт",
            "type": "object",
            "properties": {
                "role": {
                    "enum": [
                        "driver",
                        "disp",
                        "admin",
                        "mainAdmin"
                    ],
                    "allOf": [
                        {
                            "$ref": "#/definitions/model.RoleEnum"
                        }
                    ]
                }
            }
        },
        "model.LoginRequest": {
            "description": "DTO для передачи данных авторизации",
            "type": "object",
            "properties": {
                "login": {
                    "type": "string",
                    "example": "user123"
                },
                "password": {
                    "type": "string",
                    "example": "qwerty123"
                }
            }
        },
        "model.Order": {
            "description": "Модель заявки",
            "type": "object",
            "properties": {
                "cargo_description": {
                    "type": "string"
                },
                "cargo_name": {
                    "type": "string"
                },
                "cargo_type_id": {
                    "type": "integer"
                },
                "cargo_weight": {
                    "type": "number"
                },
                "completion_time": {
                    "type": "string"
                },
                "create_order_time": {
                    "type": "string"
                },
                "depart_loc": {
                    "type": "integer"
                },
                "driver_rate": {
                    "type": "integer"
                },
                "goal_loc": {
                    "type": "integer"
                },
                "id": {
                    "type": "integer"
                },
                "is_postponed": {
                    "type": "boolean"
                },
                "is_urgent": {
                    "type": "boolean"
                },
                "order_status_id": {
                    "type": "integer"
                },
                "photo_id": {
                    "type": "string"
                },
                "pickup_time": {
                    "type": "string"
                },
                "time": {
                    "type": "string"
                }
            }
        },
        "model.OrderCreate": {
            "description": "DTO для создания заявки",
            "type": "object",
            "properties": {
                "cargo_description": {
                    "type": "string"
                },
                "cargo_name": {
                    "type": "string"
                },
                "cargo_type_id": {
                    "type": "integer"
                },
                "cargo_weight": {
                    "type": "number"
                },
                "depart_loc": {
                    "type": "integer"
                },
                "goal_loc": {
                    "type": "integer"
                },
                "photo_id": {
                    "type": "string"
                },
                "time": {
                    "type": "string"
                }
            }
        },
        "model.OrderOut": {
            "description": "DTO для передачи информации о заявке",
            "type": "object",
            "properties": {
                "cargo_description": {
                    "type": "string"
                },
                "cargo_name": {
                    "type": "string"
                },
                "cargo_type_name": {
                    "type": "string"
                },
                "cargo_weight": {
                    "type": "number"
                },
                "depart_loc_name": {
                    "type": "string"
                },
                "dispatcher_name": {
                    "type": "string"
                },
                "driver_name": {
                    "type": "string"
                },
                "goal_loc_name": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "order_status_name": {
                    "$ref": "#/definitions/model.OrderStatusEnum"
                },
                "photo_id": {
                    "type": "string"
                },
                "time": {
                    "type": "string"
                }
            }
        },
        "model.OrderStatusEnum": {
            "description": "Статус заказа",
            "type": "string",
            "enum": [
                "Новый",
                "Принят",
                "Выполнен",
                "Отменен",
                "Неизвестно"
            ],
            "x-enum-varnames": [
                "NEW",
                "ACCEPT",
                "COMPLETE",
                "CANCEL",
                "UNKNOWN"
            ]
        },
        "model.OrdersResponse": {
            "description": "Ответ со списком заявок",
            "type": "object",
            "properties": {
                "count": {
                    "type": "integer"
                },
                "orders": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.OrderOut"
                    }
                }
            }
        },
        "model.RegisterRequest": {
            "description": "DTO для передачи данных регистрации",
            "type": "object",
            "properties": {
                "fio": {
                    "type": "string",
                    "example": "Иванов Иван Иванович"
                },
                "login": {
                    "type": "string",
                    "example": "user123"
                },
                "password": {
                    "type": "string",
                    "example": "qwerty123"
                },
                "phone": {
                    "type": "string",
                    "example": "+79998887766"
                },
                "role": {
                    "allOf": [
                        {
                            "$ref": "#/definitions/model.RoleEnum"
                        }
                    ],
                    "example": "driver"
                },
                "role_password": {
                    "type": "string",
                    "example": "qwerty123"
                }
            }
        },
        "model.RoleEnum": {
            "description": "Роль пользователя в системе",
            "type": "string",
            "enum": [
                "driver",
                "disp",
                "admin",
                "mainAdmin"
            ],
            "x-enum-varnames": [
                "DRIVER",
                "DISP",
                "ADMIN",
                "MAIN_ADMIN"
            ]
        },
        "model.TL_identety_ModelUser": {
            "type": "object",
            "properties": {
                "fio": {
                    "type": "string"
                },
                "internal_id": {
                    "type": "integer"
                },
                "is_denied": {
                    "type": "boolean"
                },
                "login": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "phone": {
                    "type": "string"
                },
                "public_id": {
                    "type": "string"
                }
            }
        },
        "model.UserUpdate": {
            "description": "DTO для обновления информации о пользователе",
            "type": "object",
            "properties": {
                "fio": {
                    "type": "string"
                },
                "login": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "phone": {
                    "type": "string"
                }
            }
        }
    }
}